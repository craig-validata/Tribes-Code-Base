// add the object
if($TestShape[Name] != "")
{
   focusServer();
   
   // create the objects and add to the missiongroup
   %vol = newObject(shapeVol, SimVolume, $TestShape[Name] @ ".div", true);
   addToSet(MissionGroup, %vol);
   
   %shape = newObject(testShape, InteriorShape, $TestShape[Name] @ ".dis");
   addToSet(MissionGroup, %shape);
   
   // move the shape
   GameBase::setPosition(%shape, $TestShape[XPos] @ " " @
      $TestShape[YPos] @ " " @ $TestShape[ZPos]);

}